
<section id="banner">
  <div class="content">
    <header>
      <h1>Hi <%=dataUser.uName%>, Welcome to PayMyProject<br />
      </h1>
      <p>A fully responsive site to support your favorite content/project creators</p>
    </header>
    <p>BELOW ARE YOUR FOLLOWED PROJECTS</p>
  </div>

</section>
<section>
  <header class="major">
    <h2>Current Open Projects</h2> <!-- Change to Following if following-->
  </header>
  <div class="posts">

    <% msg.forEach(function(item) { %>
    <article>
      <a class="image test"><img src=<%= item.Project.img %> alt="" /></a>
      <h3><%= item.Project.name %></h3>
      <p><%= item.Project.desc %></p>
      <ul class="actions">
        <li><a href="/project/<%= item.Project.id %>" class="button">More</a></li>
      </ul>
    </article>
    <% }); %>
  </div>
</section>